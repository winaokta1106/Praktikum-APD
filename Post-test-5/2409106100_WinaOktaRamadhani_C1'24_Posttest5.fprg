    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="opsi, namalengkap, Username, psw, alamat" type="String" array="False" size=""/>
            <declare name="nik" type="Integer" array="False" size=""/>
            <output expression="&quot;Selamat datang di Aplikasi Pendaftaran Pelatihan Kerja! &quot;" newline="True"/>
            <output expression="&quot;Silahkan pilih Daftar Akun Jika Belum Punya Akun &amp; Jika Sudah Memiliki Akun Silahkan Login : &quot;" newline="True"/>
            <output expression="&quot;1. Daftar Akun, 2. Login, 3. Exit &quot;" newline="True"/>
            <input variable="opsi"/>
            <if expression="opsi=1">
                <then>
                    <output expression="&quot;Hallo Pengguna Baru? Ayo Buat Akun Dulu!&quot;" newline="True"/>
                    <output expression="&quot;Nama Lengkap : &quot;" newline="True"/>
                    <input variable="namaLengkap"/>
                    <output expression="&quot;User Name : &quot;" newline="True"/>
                    <input variable="UserName"/>
                    <output expression="&quot;Password : &quot;" newline="True"/>
                    <input variable="psw"/>
                    <output expression="&quot;Nomor Induk Kependudukan (NIK) :&quot;" newline="True"/>
                    <input variable="nik"/>
                    <output expression="&quot;Alamat Domisili Sesuai KTP : &quot;" newline="True"/>
                    <input variable="alamat"/>
                </then>
                <else>
                    <if expression="opsi=2">
                        <then>
                            <output expression="&quot;Silahkan Login dengan User Anda : &quot;" newline="True"/>
                        </then>
                        <else>
                            <if expression="opsi=3">
                                <then>
                                    <output expression="&quot;Silahkan Keluar dari System !&quot;" newline="True"/>
                                </then>
                                <else/>
                            </if>
                        </else>
                    </if>
                </else>
            </if>
        </body>
